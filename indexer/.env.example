# Hypnos Indexer Environment Variables

# Network RPC URL (Sepolia)
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY

# Contract Addresses (from deployment)
HYPNOS_EXECUTOR_ADDRESS=0x0000000000000000000000000000000000000000
HYPNOS_DEMO_ADDRESS=0x0000000000000000000000000000000000000000

# Start blocks (optional - set to deployment block for efficiency)
HYPNOS_EXECUTOR_START_BLOCK=0
HYPNOS_DEMO_START_BLOCK=0

# Envio API Key (for deployment)
ENVIO_API_KEY=

# Database URL (for local development)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/hypnos_indexer
